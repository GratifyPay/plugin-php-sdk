{
  "name": "gratifypay/plugin-php-sdk",
  "description": "Gratify Pay PHP SDK",
  "license": "AFL-3.0",
  "version": "1.1.0 ",
  "type": "library",
  "require": {
    "php": ">=7.3",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "GratifyPay\\PhpSdk\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Gratify Pay",
      "email": "support@gratifypay.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "archive": {
    "exclude": [
      "/test",
      "/examples",
      "/.docker",
      "/build.sh",
      "/.git",
      "/.gitignore",
      "/vendor",
      "/docker-compose.yml",
      "/*.zip"
    ]
  }
}
